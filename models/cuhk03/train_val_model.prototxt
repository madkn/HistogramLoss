  top: "conv1_1_3"
}



layer {
  name: "pool1_1_1"
  type: "Pooling"
  bottom: "conv1_1_1"
  top: "pool1_1_1"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}

layer {
  name: "pool1_1_2"
  type: "Pooling"
  bottom: "conv1_1_2"
  top: "pool1_1_2"
  pooling_param {
    pool: MAX
