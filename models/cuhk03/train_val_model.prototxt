################full connection
layer {
  name: "pool2_1"
  bottom: "pool2_1_1"
  bottom: "pool2_1_2"
  bottom: "pool2_1_3"
  top: "pool2_1"
  type: "Concat"
  concat_param {
    concat_dim: 1
  }
 
}

layer {
  name: "drop1"
  type: "Dropout"
  bottom: "pool2_1"
  top: "pool2_1"
  dropout_param {
    dropout_ratio:0.5
  }
  include: { phase: TRAIN }
}


layer {
  name: "ip1_reid"
  type: "InnerProduct"
  bottom :"pool2_1"
  top: "ip1_reid"

  inner_product_param {
    num_output: 500
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "gaussian"
      std: 0.001
    }
  }
  param {
      name :"ip1_w_reid"
 decay_mult: 1
  }
  param {
      name :"ip1_b_reid" 
 decay_mult: 0
  } 
}


